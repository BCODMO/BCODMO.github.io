---
layout: none
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>{{ site.title }}</title>
	<link href="https://blog.bco-dmo.org/atom.xml" rel="self"/>
	<link href="https://blog.bco-dmo.org"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://blog.bco-dmo.org</id>
	<author>
		<name>BCO-DMO</name>
		<email>info@bco-dmo.org</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://blog.bco-dmo.org{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://blog.bco-dmo.org{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
